{
  "name": "solidity-auditor-theme",
  "displayName": "Solidity Auditor Theme",
  "description": "Dark theme for Solidity",
  "version": "2.0.0",
  "publisher": "DarioFSanchez",
  "icon": "images/icon.png",
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/SanchezDario/Solidity-Auditor-VS-Theme"
  },
  "keywords": [
    "themes",
    "theme",
    "solidity",
    "blockchain"
  ],
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "solidity",
        "aliases": [
          "solidity",
          "solidity"
        ],
        "extensions": [
          ".sol"
        ],
        "configuration": "./src/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "solidity",
        "scopeName": "source.solidity.security",
        "path": "./src/syntaxes/solidity.tmLanguage.json",
        "injectTo": [
          "source.solidity"
        ]
      }
    ],
    "themes": [
      {
        "label": "Sol-Auditor",
        "uiTheme": "vs-dark",
        "path": "./src/themes/Sol-Auditor-color-theme.json"
      }
    ]
  }
}
